.spinner
  content ""
  position relative
  width 16px
  height 16px
  background-color rgb(21,76,121)
  border-radius 8px
  animation flowHereAndBack 1s infinite alternate

@keyframes flowHereAndBack
  0%
    animation-timing-function ease-in-out
    left 0px
  100%
    animation-timing-function ease-in-out
    left 30%